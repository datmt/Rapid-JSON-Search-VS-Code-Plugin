{
	"name": "rapid-json-search",
	"displayName": "Rapid JSON Search",
	"description": "Quickly search for JSON in current file",
	"version": "0.0.8",
	"publisher": "datmt24",
	"license": "MIT",
	"homepage": "https://binarycarpenter.com",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"icon" : "images/icon.png",
	"repository": {
		"type": "git",
		"url" : "https://github.com/datmt/Rapid-JSON-Search-VS-Code-Plugin.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"enableProposedApi": false,
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rapidJSONSearch.create",
		"onCommand:rapidJSONSearch.clear"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rapidJSONSearch.create",
				"title": "Rapid JSON Search: Create"
			},
			{
				"command": "rapidJSONSearch.clear",
				"title": "Rapid JSON Search: Clear"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "tslint -p ./"
	},
	"devDependencies": {
		"@types/node": "^6.14.9",
		"@types/vscode": "^1.43.0",
		"tslint": "^5.20.1",
		"typescript": "^3.8.3"
	}
}
